#!/bin/sh
PLUGINDIR=/usr/local/share/enigma2/NoGfx
echo "removing HDMUMod-Skin"
rm -r $PLUGINDIR > /dev/null 2>&1
rm -r /usr/lib/enigma2/python/Components/Renderer/HDMUCaids.pyo > /dev/null 2>&1
rm -r /usr/lib/enigma2/python/Components/Renderer/VolumeText.pyo > /dev/null 2>&1
rm -r /usr/lib/enigma2/python/Components/Converter/HDMUEpgList.pyo > /dev/null 2>&1
rm -r /usr/lib/enigma2/python/Components/Converter/ServiceNumber.pyo > /dev/null 2>&1
rm -r /usr/lib/enigma2/python/Components/Converter/HDMUCaidDisplay.pyo > /dev/null 2>&1
exit 0
